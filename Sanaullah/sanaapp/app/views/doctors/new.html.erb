<div class="center jumbotron">
<h1>New Doctor</h1>

<%= form_for :doctor, url: doctors_path do |d| %>
<p>
<%= d.label :Doctor_Name %><br>
<%= d.date_field :dname %>
</p>
<p>
<%= d.label :Gender %><br>
<%= d.text_field :dgender %>
</p>
<p>
<%= d.label :Contact %><br>
<%= d.text_field :dcontact %>
</p>
<p>
<%= d.label :Department %><br>
<%= d.text_field :ddept %>
</p>
<p>
<%= d.label :Patient_ID %><br>
<%= d.number_field :patient_id %>
</p>
<p>
<%= d.label :Labreport_ID %><br>
<%= d.number_field :labreport_id %>
</p>
<p>
<%= d.submit %>
</p>

<% end %>
<br>
<br>
<a href="/doctors" class="buttonsmall buttonb">Back</a>
<a href="/" class="buttonsmall buttonb">Home</a>

</div>
